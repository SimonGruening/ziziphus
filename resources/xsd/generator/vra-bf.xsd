                                                   <xsd:attribute name="extent" type="xsd:string"/>
                                                   <xsd:attribute name="href" type="xsd:anyURI"/>
                                                   <xsd:attribute ref="xml:lang"/>
                                                   <xsd:attribute name="pref" type="xsd:boolean"/>
                                                   <xsd:attribute name="refid" type="xsd:string"/>
                                                   <xsd:attribute name="rules" type="xsd:string"/>
                                                   <xsd:attribute name="source" type="xsd:string"/>
                                                   <xsd:attribute name="vocab" type="xsd:string"/>
                                                   <xsd:attribute name="lang" type="langType"/>
                                                   <xsd:attribute name="transliteration" type="xsd:string"/>
                                                   <xsd:attribute name="script" type="scriptType"/>
                                                   <xsd:attribute name="lastAccessed" type="xsd:date"/>
                                                </xsd:extension>
                                             </xsd:simpleContent>
                                          </xsd:complexType>
                                       </xsd:element>
                                       <xsd:element name="culture" minOccurs="0">
                                          <xsd:complexType>
                                             <xsd:annotation>
                                                <xsd:documentation>Basic datatype for all freetext values. Allows assignment of source, vocabulary, refid and language attributes to virtually any value.</xsd:documentation>
                                             </xsd:annotation>
                                             <xsd:simpleContent>
                                                <xsd:extension base="xsd:string">
                                                   <xsd:attribute name="dataDate" type="dateValueType"/>
                                                   <xsd:attribute name="extent" type="xsd:string"/>
                                                   <xsd:attribute name="href" type="xsd:anyURI"/>
                                                   <xsd:attribute ref="xml:lang"/>
                                                   <xsd:attribute name="pref" type="xsd:boolean"/>
                                                   <xsd:attribute name="refid" type="xsd:string"/>
                                                   <xsd:attribute name="rules" type="xsd:string"/>
                                                   <xsd:attribute name="source" type="xsd:string"/>
                                                   <xsd:attribute name="vocab" type="xsd:string"/>
                                                   <xsd:attribute name="lang" type="langType"/>
                                                   <xsd:attribute name="transliteration" type="xsd:string"/>
                                                   <xsd:attribute name="script" type="scriptType"/>
                                                   <xsd:attribute name="lastAccessed" type="xsd:date"/>
                                                </xsd:extension>
                                             </xsd:simpleContent>
                                          </xsd:complexType>
                                       </xsd:element>
                                       <xsd:element name="dates" minOccurs="0">
                                          <xsd:complexType>
                                             <xsd:sequence>
                                                <xsd:element name="earliestDate" minOccurs="0">
                                                   <xsd:complexType>
                                                      <xsd:annotation>
                                                         <xsd:documentation>Basic datatype for all date elements. Allows a textual element body and a "circa" attribute.</xsd:documentation>
                                                      </xsd:annotation>
                                                      <xsd:simpleContent>
                                                         <xsd:extension base="dateValueType">
                                                            <xsd:attribute name="circa" type="xsd:boolean"/>
                                                         </xsd:extension>
                                                      </xsd:simpleContent>
                                                   </xsd:complexType>
                                                </xsd:element>
                                                <xsd:element name="latestDate" minOccurs="0">
                                                   <xsd:complexType>
                                                      <xsd:annotation>
                                                         <xsd:documentation>Basic datatype for all date elements. Allows a textual element body and a "circa" attribute.</xsd:documentation>
                                                      </xsd:annotation>
                                                      <xsd:simpleContent>
                                                         <xsd:extension base="dateValueType">
                                                            <xsd:attribute name="circa" type="xsd:boolean"/>
                                                         </xsd:extension>
                                                      </xsd:simpleContent>
                                                   </xsd:complexType>
                                                </xsd:element>
                                             </xsd:sequence>
                                             <xsd:attribute name="type" type="agentDateTypeType"/>
                                          </xsd:complexType>
                                       </xsd:element>
                                       <xsd:element name="name" minOccurs="0">
                                          <xsd:complexType>
                                             <xsd:annotation>
                                                <xsd:documentation>Agent name element. Use type attribute to differentiate different kinds of names (personal, corporate, etc.).</xsd:documentation>
                                             </xsd:annotation>
                                             <xsd:simpleContent xmlns="http://www.w3.org/2001/XMLSchema">
                                                <xsd:extension base="xsd:string">
                                                   <xsd:attribute xmlns="http://www.vraweb.org/vracore4.htm" name="type" type="agentNameTypeType"/>
                                                   <xsd:attribute xmlns="http://www.vraweb.org/vracore4.htm" name="dataDate" type="dateValueType"/>
                                                   <xsd:attribute xmlns="http://www.vraweb.org/vracore4.htm" name="extent" type="xsd:string"/>
                                                   <xsd:attribute xmlns="http://www.vraweb.org/vracore4.htm" name="href" type="xsd:anyURI"/>
                                                   <xsd:attribute xmlns="http://www.vraweb.org/vracore4.htm" ref="xml:lang"/>
                                                   <xsd:attribute xmlns="http://www.vraweb.org/vracore4.htm" name="pref" type="xsd:boolean"/>
                                                   <xsd:attribute xmlns="http://www.vraweb.org/vracore4.htm" name="refid" type="xsd:string"/>
                                                   <xsd:attribute xmlns="http://www.vraweb.org/vracore4.htm" name="rules" type="xsd:string"/>
                                                   <xsd:attribute xmlns="http://www.vraweb.org/vracore4.htm" name="source" type="xsd:string"/>
                                                   <xsd:attribute xmlns="http://www.vraweb.org/vracore4.htm" name="vocab" type="xsd:string"/>
                                                   <xsd:attribute xmlns="http://www.vraweb.org/vracore4.htm" name="lang" type="langType"/>
                                                   <xsd:attribute xmlns="http://www.vraweb.org/vracore4.htm" name="transliteration"
                                                                  type="xsd:string"/>
                                                   <xsd:attribute xmlns="http://www.vraweb.org/vracore4.htm" name="script" type="scriptType"/>
                                                   <xsd:attribute xmlns="http://www.vraweb.org/vracore4.htm" name="lastAccessed" type="xsd:date"/>
                                                </xsd:extension>
                                             </xsd:simpleContent>
                                          </xsd:complexType>
                                       </xsd:element>
                                       <xsd:element name="role" minOccurs="0">
                                          <xsd:complexType>
                                             <xsd:annotation>
                                                <xsd:documentation>Basic datatype for all roles.</xsd:documentation>
                                             </xsd:annotation>
                                             <xsd:simpleContent>
                                                <xsd:extension base="xsd:string">
                                                   <xsd:attribute name="type" type="roleString"/>
                                                </xsd:extension>
                                             </xsd:simpleContent>
                                          </xsd:complexType>
                                       </xsd:element>
                                    </xsd:sequence>
                                    <xsd:attribute name="dataDate" type="dateValueType"/>
                                    <xsd:attribute name="extent" type="xsd:string"/>
                                    <xsd:attribute name="href" type="xsd:anyURI"/>
                                    <xsd:attribute ref="xml:lang"/>
                                    <xsd:attribute name="pref" type="xsd:boolean"/>
                                    <xsd:attribute name="refid" type="xsd:string"/>
